{
    "name": "reposit",
    "version": "1.0.7",
    "description": "",
    "main": "./lib/index.js",
    "bin": {
        "reposit": "./lib/index.js"
    },
    "files": [
        "/lib/**/*",
        "data/"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/11grossmane/reposit.git"
    },
    "scripts": {
        "create": "npm run build && npm run global",
        "build": "npm run clean && tsc -p .",
        "global": "npm i -g",
        "github-reset": "ts-node ./src/index.ts -g -r",
        "github": "ts-node ./src/index.ts -g",
        "bitbucket": "ts-node ./src/index.ts -b",
        "bitbucket-reset": "ts-node ./src/index.ts -b -r",
        "clean": "find ./lib ! -name '.gitignore' -type f -exec rm -f {} + && rm -f ./data/login.yaml",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
        "push-both": "git push bitbucket master && git push",
        "test": "npm run build",
        "build-publish": "npm run build && npm publish"
    },
    "keywords": [],
    "author": "11grossmane",
    "license": "MIT",
    "dependencies": {
        "axios": "^0.19.2",
        "chalk": "^4.0.0",
        "commander": "^5.0.0",
        "figlet": "^1.3.0",
        "inquirer": "^7.1.0",
        "js-yaml": "^3.13.1",
        "slugify": "^1.4.0",
        "ts-node": "^8.8.2",
        "typescript": "^3.8.3"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/chalk": "^2.2.0",
        "@types/commander": "^2.12.2",
        "@types/figlet": "^1.2.0",
        "@types/inquirer": "^6.5.0",
        "@types/jest": "^25.2.1",
        "@types/js-yaml": "^3.12.3",
        "@types/node": "^13.13.0",
        "@types/typescript": "^2.0.0",
        "@types/yaml": "^1.2.0",
        "jest": "^25.4.0",
        "ts-jest": "^25.4.0"
    }
}
